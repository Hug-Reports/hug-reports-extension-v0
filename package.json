{
  "name": "gratitude",
  "displayName": "gratitude",
  "description": "Give credit to open source developers",
  "version": "0.0.1",
  "files": [
    "images/*.png"
  ],
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:python",
    "onLanguage:typescript",
    "onLanguage:javascript"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gratitude.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "gratitude.sayThanks",
        "title": "Say Thanks"
      },
      {
        "command": "gratitude.sayMore",
        "title": "Say More"
      },
      {
        "command": "gratitude.fillOutSurvey",
        "title": "Fill Out Survey"
      }
    ],
    "menus": {
      "editor/lineNumber/context": [
        {
          "command": "gratitude.sayThanks",
          "title": "Say Thanks",
          "group": "navigation",
          "when": "gratitude.hasImport"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.79.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "@google-cloud/local-auth": "^2.1.0",
    "@vscode/vsce": "^2.19.0",
    "googleapis": "^105.0.0",
    "mongodb": "^5.7.0",
    "vsce": "^2.15.0"
  }
}
